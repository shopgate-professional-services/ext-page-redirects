{
  "version": "1.1.0",
  "id": "@shopgate-project/page-redirects",
  "components": [
    {
      "id": "subscribers",
      "path": "frontend/redirects/subscriptions.js",
      "type": "subscribers"
    },
    {
      "id": "config",
      "path": "frontend/config/reducers.js",
      "type": "reducers"
    }
  ],
  "configuration": {
    "redirects": {
      "type": "admin",
      "destination": "frontend",
      "default": null,
      "params": {
        "label": "Static configuration for redirects",
        "type": "json"
      }
    },
    "configEndpoint": {
      "type": "admin",
      "destination": "backend",
      "params": {
        "type": "text",
        "label": "External endpoint (url) to pull page redirects configuration"
      },
      "default": "https://www.dropbox.com/scl/fi/fvoeeaw48u0cpj1yuu4bp/redirects.json?rlkey=eab5ga5roxvwff5z3r7l1f60j&st=sr8gajob&dl=1"
    },
    "configTTL": {
      "type": "admin",
      "destination": "both",
      "params": {
        "type": "text",
        "label": "TTL (sec) to cache configuration"
      },
      "default": 3600
    }
  },
  "steps": []
}
